# Porta local do servidor de overlay (OBS Browser Source)
OVERLAY_PORT=8080

# Endpoint websocket da origem dos lotes (payload no objeto raiz)
WS_URL=ws://localhost:3000

# Tempo para marcar dados como "stale" na tela
STALE_MS=15000

# Reconnect com backoff exponencial + jitter
RECONNECT_MIN_MS=1000
RECONNECT_MAX_MS=30000

# NÃ­vel de logs estruturados (fatal,error,warn,info,debug,trace)
LOG_LEVEL=info
